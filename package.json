{
    "name": "seoul-data-chart-app",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "json-server --watch db.json --port 4000 & vite",
        "build": "GENERATE_SOURCEMAP=false && tsc && vite build",
        "preview": "vite preview",
        "postinstall": "husky install",
        "format": "prettier --cache --write .",
        "lint": "eslint --cache .",
        "buildCommand": "next build"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": "npm run format"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    },
    "dependencies": {
        "axios": "^1.5.0",
        "json-server": "^0.17.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^4.11.0",
        "react-router-dom": "^6.15.0",
        "recharts": "^2.8.0",
        "styled-components": "^6.0.7"
    },
    "devDependencies": {
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.7.0",
        "@vitejs/plugin-react": "^4.0.3",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-import-resolver-typescript": "^3.6.0",
        "eslint-plugin-import": "^2.28.1",
        "husky": "^8.0.3",
        "prettier": "^3.0.2",
        "typescript": "^5.0.2",
        "vite": "^4.4.5"
    }
}
